{
  "name": "Pivotics",
  "description": "A pivot table editor",
  "version": "12.2.0.0",
    "manifest_version": 2,
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "app": {
    "launch": {
      "local_path": "app/pivotics.html"
    }
  },
  "icons": {
    "128": "chromeapp/icon_128.png",
    "16": "chromeapp/icon_16.png"
  },
   "permissions": [
    "unlimitedStorage",
    "notifications"
  ]
}